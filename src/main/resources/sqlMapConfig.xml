<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-config.dtd">
  
  <configuration>
  
  	<!-- Alias 설정 -->
  	<typeAliases>
  		<typeAlias alias="todolist" type="com.springproj.biz.domain.TodolistVO"/>
  		<typeAlias alias="users" type="com.springproj.biz.domain.UsersVO"/>
  	</typeAliases>
  	
  	<!-- sql Mapper 설정 -->
  	<mappers>
  		<mapper resource="mappings/todolistMapping.xml"/>
  		<mapper resource="mappings/usersMapping.xml"/>
  	</mappers>
  
  </configuration>